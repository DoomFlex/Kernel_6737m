#include <generated/autoconf.h>
#ifdef CONFIG_MTK_HAFG_20
#include "vz6737t_66_i_m0_fg20_bat_setting.dtsi"
#else
#include "vz6737t_66_i_m0_fg_oldgauge_bat_setting.dtsi"
